{
  "name": "react-router-base",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "imports": {
    "@/*": "./app/*"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:react-router": "prisma generate && react-router build",
    "build:server": "tsx ./other/build-server.ts",
    "dev": "cross-env NODE_ENV=development MOCKS=true node ./server/dev-server.js",
    "dev:no-mocks": "cross-env NODE_ENV=development node ./server/dev-server.js",
    "docker": "docker compose -f ./other/docker-compose.yml up -d",
    "format": "prettier --write .",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=production node .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "typecheck": "react-router typegen && tsc",
    "validate": "run-p lint typecheck",
    "prepare": "husky"
  },
  "prettier": "@epic-web/config/prettier",
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/server-build"
  ],
